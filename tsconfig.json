{
  "compilerOptions": {
    "baseUrl": ".",
    "outDir": "dist",
    "noImplicitThis": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "target": "ESNext",
    "module": "ESNext",
    "lib": ["ESNext"],
    "moduleResolution": "node",
    "jsx": "react-jsx",
    "strict": true
  }
}
